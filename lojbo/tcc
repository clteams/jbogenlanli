S <- S + t {$$=new opconn(j,S,t);}
   / t {$$=t;}
S <- t * S {/*j->modify(n,st);*/$$=new opconn(j,t,S);}
   / t {$$=t;}
S <- t + tag:stag? BO S {$$=new opconn(j->modify(NULL,T,f,B),t,S);}

S <- t BO S {$$=new opconn(NULL,t,S);}

mex:
S <- S '+' t; {$$=new opconn(op,S,t);
   / t {$$=t;}

